    				<input type="text" class="input-sm form-control filter-date" id="end-date" placeholder="end Date">
    			</div>
          
          <span style="display:inline-block; width: 80px;"></span>
          
          <div id="filter-location" class= "filter">
            <span style="display:inline-block; width: 10px;" class= "filter-title">City</span>
            <span style="display:inline-block; width: 30px;" class= "filter-title"></span>
            <input id="address" type="textbox" value="San Francisco, CA" class="form-control">
          </div>

